
-var navArray = "Объекты; Компания; Как купить; Агентам; Регионам; Акции; Контакты".split('; ')

header.header

    nav.header__nav
        img.header__logo(src=require(`./logo.svg`) alt="Логотип")
        .header__menu
            i(style="font-size: 36px").material-icons menu
        ul.header__list
            each elem in navArray
                li
                    a.header__anchor(href="")= elem
    .header__contact
        p.header__number 8 888 88 88 98
        p.header__text Звони скорее!